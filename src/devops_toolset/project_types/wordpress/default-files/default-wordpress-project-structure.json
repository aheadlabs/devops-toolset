{  "$schema": "http://dev.aheadlabs.com/schemas/json/project-structure-schema.json",  "items": [    {      "name": ".devops",      "type": "directory",      "children": [        {          "name": ".gitkeep",          "type": "file",          "condition": "when-parent-not-empty"        }      ]    },    {      "name": ".media",      "type": "directory",      "children": [        {          "name": ".gitkeep",          "type": "file",          "condition": "when-parent-not-empty"        }      ]    },    {      "name": ".run",      "type": "directory",      "children": [        {          "name": "npm.run.build.xml",          "type": "file",          "default_content": {            "source": "from_library",            "value": "default-jetbrains-run-wordpress-theme-build.xml"          }        },        {          "name": "npm.run.build-production.xml",          "type": "file",          "default_content": {            "source": "from_library",            "value": "default-jetbrains-run-wordpress-theme-build-production.xml"          }        },        {          "name": "npm.run.watch.xml",          "type": "file",          "default_content": {            "source": "from_library",            "value": "default-jetbrains-run-wordpress-theme-watch.xml"          }        },        {          "name": "node.debug.theme.webpack.xml",          "type": "file",          "default_content": {            "source": "from_library",            "value": "default-jetbrains-debug-wordpress-theme-webpack.xml"          }        }      ]    },    {      "name": "content",      "type": "directory",      "children": [        {          "name": "auth0",          "type": "directory",          "children": [            {              "name": ".gitkeep",              "type": "file",              "condition": "when-parent-not-empty"            }          ]        },        {          "name": "plugins",          "type": "directory",          "children": [            {              "name": ".gitkeep",              "type": "file",              "condition": "when-parent-not-empty"            }          ]        },        {          "name": "pre-website",          "type": "directory",          "children": [            {              "name": ".gitkeep",              "type": "file",              "condition": "when-parent-not-empty"            }          ]        },        {          "name": "themes",          "type": "directory",          "children": [            {              "name": ".gitkeep",              "type": "file",              "condition": "when-parent-not-empty"            }          ]        },        {          "name": "wxr",          "type": "directory",          "children": [            {              "name": "mapping.csv",              "type": "file",              "condition": "when-parent-not-empty"            }          ]        }      ]    },    {      "name": "database",      "type": "directory",      "children": [        {          "name": ".gitkeep",          "type": "file",          "condition": "when-parent-not-empty"        }      ]    },    {      "name": "wordpress",      "type": "directory",      "children": [        {          "name": ".gitkeep",          "type": "file",          "condition": "when-parent-not-empty"        }      ]    },    {      "name": ".gitignore",      "type": "file",      "default_content": {        "source": "from_library",        "value": "default.gitignore"      }    },    {      "name": "project.xml",      "type": "file",      "default_content": {        "source": "from_library",        "value": "default-project.xml"      }    },    {      "name": "README.md",      "type": "file",      "default_content": {        "source": "from_library",        "value": "default-README.md"      }    }  ]}