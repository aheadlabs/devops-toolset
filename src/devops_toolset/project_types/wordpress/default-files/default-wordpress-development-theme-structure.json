{
  "$schema": "https://dev.aheadlabs.com/schemas/json/project-structure-schema.json",
  "items": [
      {
          "name": "{{mytheme}}",
          "type": "directory",
          "children": [
              {
                  "name": ".devops",
                  "type": "directory",
                  "children": [
                      {
                          "name": ".gitkeep",
                          "type": "file",
                          "condition": "when-parent-not-empty"
                      }
                  ]
              },
              {
                  "name": "src",
                  "type": "directory",
                  "children": [
                      {
                          "name": "assets",
                          "type": "directory",
                          "children": [
                              {
                                  "name": "css",
                                  "type": "directory",
                                  "children": [
                                      {
                                          "name": "style.scss",
                                          "type": "file",
                                          "default_content": {
                                              "source": "from_library",
                                              "value": "default-style.scss"
                                          }
                                      },
                                      {
                                          "name": "_variables.scss",
                                          "type": "file",
                                          "default_content": {
                                              "source": "from_library",
                                              "value": "default-variables.scss"
                                          }
                                      }
                                  ]
                              },
                              {
                                  "name": "fonts",
                                  "type": "directory",
                                  "children": [
                                      {
                                          "name": ".gitkeep",
                                          "type": "file",
                                          "condition": "when-parent-not-empty"
                                      }
                                  ]
                              },
                              {
                                  "name": "images",
                                  "type": "directory",
                                  "children": [
                                      {
                                          "name": "favicon.png",
                                          "type": "bfile",
                                          "default_content": {
                                              "source": "from_library",
                                              "value": "default-favicon.png"
                                          }
                                      }
                                  ]
                              },
                              {
                                  "name": "ts",
                                  "type": "directory",
                                  "children": [
                                      {
                                          "name": "main.ts",
                                          "type": "file",
                                          "default_content": {
                                              "source": "from_library",
                                              "value": "default-main.ts"
                                          }
                                      }
                                  ]
                              }
                          ]
                      },
                      {
                          "name": "functions_php",
                          "type": "directory",
                          "children": [
                              {
                                  "name": "_core.php",
                                  "type": "file",
                                  "default_content": {
                                      "source": "from_library",
                                      "value": "default-functions-core.php"
                                  }
                              },
                              {
                                  "name": "_toolset.php",
                                  "type": "file",
                                  "default_content": {
                                      "source": "from_library",
                                      "value": "default-functions-toolset.php"
                                  }
                              }
                          ]
                      },
                      {
                          "name": "languages",
                          "type": "directory",
                              "children": [
                                  {
                                      "name": ".gitkeep",
                                      "type": "file",
                                      "condition": "when-parent-not-empty"
                                  }
                              ]
                      },
                      {
                          "name": "page-templates",
                          "type": "directory",
                              "children": [
                                  {
                                      "name": ".gitkeep",
                                      "type": "file",
                                      "condition": "when-parent-not-empty"
                                  }
                              ]
                      },
                      {
                          "name": "template-parts",
                          "type": "directory",
                              "children": [
                                  {
                                      "name": ".gitkeep",
                                      "type": "file",
                                      "condition": "when-parent-not-empty"
                                  }
                              ]
                      },
                      {
                          "name": "woocommerce",
                          "type": "directory",
                              "children": [
                                  {
                                      "name": ".gitkeep",
                                      "type": "file",
                                      "condition": "when-parent-not-empty"
                                  }
                              ]
                      },
                      {
                          "name": "404.php",
                          "type": "file",
                          "default_content": {
                              "source": "from_library",
                              "value": "default-404.php"
                          }
                      },
                      {
                          "name": "composer.json",
                          "type": "file",
                          "default_content": {
                              "source": "from_library",
                              "value": "default-composer.json"
                          }
                      },
                      {
                          "name": "footer.php",
                          "type": "file",
                          "default_content": {
                              "source": "from_library",
                              "value": "default-footer.php"
                          }
                      },
                      {
                          "name": "front-page.php",
                          "type": "file",
                          "default_content": {
                              "source": "from_library",
                              "value": "default-front-page.php"
                          }
                      },
                      {
                          "name": "functions.php",
                          "type": "file",
                          "default_content": {
                              "source": "from_library",
                              "value": "default-functions.php"
                          }
                      },
                      {
                          "name": "header.php",
                          "type": "file",
                          "default_content": {
                              "source": "from_library",
                              "value": "default-header.php"
                          }
                      },
                      {
                          "name": "index.php",
                          "type": "file",
                          "default_content": {
                              "source": "from_library",
                              "value": "default-index.php"
                          }
                      }
                  ]
              },
              {
                  "name": ".gitignore",
                  "type": "file",
                  "default_content": {
                      "source": "from_library",
                      "value": "default-theme-development.gitignore"
                  }
              },
              {
                  "name": "browsersync.config.js",
                  "type": "file",
                  "default_content": {
                      "source": "from_library",
                      "value": "default-browsersync.config.js"
                  }
              },
              {
                  "name": "package.json",
                  "type": "file",
                  "default_content": {
                      "source": "from_library",
                      "value": "default-theme-development-package.json"
                  }
              },
              {
                  "name": "README.md",
                  "type": "file",
                  "default_content": {
                      "source": "from_library",
                      "value": "default-theme-development-README.md"
                  }
              },
              {
                  "name": "screenshot.png",
                  "type": "bfile",
                  "default_content": {
                      "source": "from_library",
                      "value": "default-theme-development-screenshot.png"
                  }
              },
              {
                  "name": "screenshot.psd",
                  "type": "bfile",
                  "default_content": {
                      "source": "from_library",
                      "value": "default-theme-development-screenshot.psd"
                  }
              },
              {
                  "name": "tsconfig.json",
                  "type": "file",
                  "default_content": {
                      "source": "from_library",
                      "value": "default-tsconfig.json"
                  }
              },
              {
                  "name": "webpack.config.js",
                  "type": "file",
                  "default_content": {
                      "source": "from_library",
                      "value": "default-webpack.config.js"
                  }
              }
          ]
      }
  ]
}
