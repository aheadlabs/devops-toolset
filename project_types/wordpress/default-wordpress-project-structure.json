{  "$schema": "http://dev.aheadlabs.com/schemas/json/project-structure-schema.json",  "items": [    {      "name": ".devops",      "type": "directory",      "children": [        {          "name": ".gitkeep",          "type": "file",          "condition": "when-parent-not-empty"        }      ]    },    {      "name": ".media",      "type": "directory",      "children": [        {          "name": ".gitkeep",          "type": "file",          "condition": "when-parent-not-empty"        }      ]    },    {      "name": "content",      "type": "directory",      "children": [        {          "name": "auth0",          "type": "directory",          "children": [            {              "name": ".gitkeep",              "type": "file",              "condition": "when-parent-not-empty"            }          ]        },        {          "name": "plugins",          "type": "directory",          "children": [            {              "name": ".gitkeep",              "type": "file",              "condition": "when-parent-not-empty"            }          ]        },        {          "name": "pre-website",          "type": "directory",          "children": [            {              "name": ".gitkeep",              "type": "file",              "condition": "when-parent-not-empty"            }          ]        }0,        {          "name": "themes",          "type": "directory",          "children": [            {              "name": "theme-src-development",              "type": "directory",              "children": [                {                  "name": "src",                  "type": "directory",                  "children": [                    {                      "name": "gulpfile.js",                      "type": "file",                      "default_content": {                        "source": "from_url",                        "value": "https://raw.githubusercontent.com/aheadlabs/gulp-w74framework/master/gulpfile-demo.js"                      }                    },                    {                      "name": "package.json",                      "type": "file",                      "default_content": {                        "source": "from_url",                        "value": "https://raw.githubusercontent.com/aheadlabs/gulp-w74framework/master/package.json"                      }                    }                  ]                },                {                  "name": "dist",                  "type": "directory",                  "children": [                    {                      "name": ".gitkeep",                      "type": "file",                      "condition": "when-parent-not-empty"                    }                  ]                }              ]            }          ]        },        {          "name": "wxr",          "type": "directory",          "children": [            {              "name": ".gitkeep",              "type": "file",              "condition": "when-parent-not-empty"            }          ]        }      ]    },    {      "name": "database",      "type": "directory",      "children": [        {          "name": ".gitkeep",          "type": "file",          "condition": "when-parent-not-empty"        }      ]    },    {      "name": "wordpress",      "type": "directory",      "children": [        {          "name": ".gitkeep",          "type": "file",          "condition": "when-parent-not-empty"        }      ]    },    {      "name": ".gitignore",      "type": "file",      "default_content": {        "source": "from_url",        "value": "https://raw.githubusercontent.com/aheadlabs/devops-toolset/master/project_types/wordpress/default.gitignore"      }    },    {      "name": "project.xml",      "type": "file",      "default_content": {        "source": "from_url",        "value": "https://raw.githubusercontent.com/aheadlabs/devops-toolset/master/project_types/wordpress/default-project.xml"      }    },    {      "name": "README.md",      "type": "file",      "default_content": {        "source": "from_url",        "value": "https://raw.githubusercontent.com/aheadlabs/devops-toolset/master/project_types/wordpress/default-README.md"      }    }  ]}